package emc.menus.debtors.menuitems.display;

import emc.enums.enumMenuItems;
import emc.forms.debtors.display.journaldefinitions.JournalDefinitionForm;
import emc.framework.EMCMenuItem;

/**
 * @Name         : DebtorsJournalDefinitions
 *
 * @Date         : 10 May 2010
 * 
 * @Description  : Menu item for the Debtors Journal Definitions form.
 *
 * @author       : riaan
 */
public class DebtorsJournalDefinitions extends EMCMenuItem {

    /** Creates a new instance of DebtorsJournalDefinitions. */
    public DebtorsJournalDefinitions() {
        this.setClassPath(JournalDefinitionForm.class.getName());
        this.setMenuItemType(enumMenuItems.DISPLAY);
        this.setMenuItemName("Journal Definitions");
        this.setToolTipText("Set up and change Debtors journal definitions.");
    }
}
