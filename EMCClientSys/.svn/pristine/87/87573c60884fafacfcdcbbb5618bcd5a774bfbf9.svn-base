package emc.app.components.hr;

import emc.app.components.emcJComboBox;
import emc.enums.hr.HRDependantsTypes;

/**
 *
 * @author wikus
 */
public class HRDependantsTypesDropDown extends emcJComboBox {

    public HRDependantsTypesDropDown() {
        super();
        HRDependantsTypes[] types = HRDependantsTypes.values();

        for (HRDependantsTypes type : types) {
            this.addItem(type.toString());
        }
    }
}
