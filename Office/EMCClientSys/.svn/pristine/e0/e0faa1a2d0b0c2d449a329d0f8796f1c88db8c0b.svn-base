package emc.app.components.hr;

import emc.app.components.emcJComboBox;
import emc.enums.hr.HREducationTypes;

/**
 *
 * @author wikus
 */
public class HREducationTypesDropDown extends emcJComboBox {

    public HREducationTypesDropDown() {
        super();
        HREducationTypes[] types = HREducationTypes.values();

        for (HREducationTypes type : types) {
            this.addItem(type.toString());
        }
    }
}
