package emc.menus.inventory.menuitems.display;

import emc.enums.enumMenuItems;
import emc.forms.inventory.display.additionaldimensions.AdditionalDimensionsForm;
import emc.framework.EMCMenuItem;

/**
 * @Name         : AdditionalDimensions
 *
 * @Date         : 26 Jun 2009
 * 
 * @Description  : Menu item for the additional dimensions form.
 *
 * @author       : riaan
 */
public class AdditionalDimensions extends EMCMenuItem {

    /** Creates a new instance of AdditionalDimensions. */
    public AdditionalDimensions() {
        this.setClassPath(AdditionalDimensionsForm.class.getName());
        this.setMenuItemType(enumMenuItems.DISPLAY);
        this.setMenuItemName("Additional Dimensions");
        this.setToolTipText("Edit Additional Dimensions");
    }
}
