/**
 *
 * @author riaan
 */
public class POPPostingMenu extends EMCMenu {
    
    private Posting posting = new Posting();
    private PostSerialBatchMenu serialbatch = new PostSerialBatchMenu();

    /** Creates a new instance of POPPostingMenu */
