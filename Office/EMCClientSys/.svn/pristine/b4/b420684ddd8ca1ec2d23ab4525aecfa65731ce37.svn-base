package emc.menus.base.menuItems.display;

import emc.enums.enumMenuItems;
import emc.forms.base.display.messages.BaseMessagesForm;
import emc.framework.EMCMenuItem;

/**
 * @name        Messages
 *
 * @date        20 Apr 2009
 *
 * @desciption  Menu item for Messages form.
 *
 * @author      riaan
 */
public class Messages extends EMCMenuItem {

    /** Creates a new instance of Messages. */
    public Messages() {
        this.setClassPath(BaseMessagesForm.class.getName());
        this.setMenuItemType(enumMenuItems.DISPLAY);
        this.setMenuItemName("Messages");
        this.setToolTipText("Edit EMC Messages");
    }
}
