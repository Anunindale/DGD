package emc.forms.app;

/**
 * 
 * @author wikus
 */
public class ActiveDimColumnData {

    private boolean dimention1 = false;
    private boolean dimention2 = false;
    private boolean dimention3 = false;
    private boolean warehouse = false;
    private boolean batch = false;
    private boolean serialNo = false;
    private boolean location = false;
    private boolean pallet = false;

    public ActiveDimColumnData() {
    }

    public boolean isDimention1() {
        return dimention1;
    }

    public void setDimention1(boolean dimention1) {
        this.dimention1 = dimention1;
    }

    public boolean isDimention2() {
        return dimention2;
    }

    public void setDimention2(boolean dimention2) {
        this.dimention2 = dimention2;
    }

    public boolean isDimention3() {
        return dimention3;
    }

    public void setDimention3(boolean dimention3) {
        this.dimention3 = dimention3;
    }

    public boolean isBatch() {
        return batch;
    }

    public void setBatch(boolean batch) {
        this.batch = batch;
    }

    public boolean isLocation() {
        return location;
    }

    public void setLocation(boolean location) {
        this.location = location;
    }

    public boolean isPallet() {
        return pallet;
    }

    public void setPallet(boolean pallet) {
        this.pallet = pallet;
    }

    public boolean isSerialNo() {
        return serialNo;
    }

    public void setSerialNo(boolean serialNo) {
        this.serialNo = serialNo;
    }

    public boolean isWarehouse() {
        return warehouse;
    }

    public void setWarehouse(boolean warehouse) {
        this.warehouse = warehouse;
    }
}
