package emc.menus.inventory.menuitems.display;

import emc.enums.enumMenuItems;
import emc.forms.inventory.display.journaldefinitions.JournalDefinitionForm;
import emc.framework.EMCMenuItem;

/**
 *
 * @author riaan
 */
public class JournalDefinitions extends EMCMenuItem {

    /** Creates a new instance of JournalDefinitions */
    public JournalDefinitions() {
        this.setClassPath(JournalDefinitionForm.class.getName());
        this.setMenuItemType(enumMenuItems.DISPLAY);
        this.setMenuItemName("Journal Definitions");
        this.setToolTipText("Set up and change Inventory journal definitions.");
    }
}
