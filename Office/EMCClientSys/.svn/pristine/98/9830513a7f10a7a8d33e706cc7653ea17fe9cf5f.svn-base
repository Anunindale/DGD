package emc.menus.base.menuItems.display;

import emc.enums.enumMenuItems;
import emc.framework.EMCMenuItem;

/**
 *
 * @author rico
 */
public class FormInformationMenu extends EMCMenuItem {
     public FormInformationMenu(){
        this.setClassPath(emc.forms.base.display.forminformation.FormInformation.class.getName());
        this.setMenuItemType(enumMenuItems.DISPLAY);
        this.setMenuItemName("View Form Info");
        this.setToolTipText("View the current Form information");
        this.setDoNotOpenForm(true);
    }

}
