package emc.app.components.emctable.stock;

/**
 *
 * @author wikus
 */
public class StockDRMParameters {

    private String itemIdField;
    private String transIdField;
    private String dimension1Field;
    private String dimension2Field;
    private String dimension3Field;
    private String serialField;
    private String batchField;
    private String warehouseField;
    private String locationField;
    //Used to update item lookup relation manager when item reference is changed.  Default may be changed.
    private String itemReferenceField = "itemReference";

    public StockDRMParameters() {
    }

    public StockDRMParameters(String itemIdField, String dimension1Field, String dimension2Field, String dimension3Field, String transIdField) {
        this.itemIdField = itemIdField;
        this.transIdField = transIdField;
        this.dimension1Field = dimension1Field;
        this.dimension2Field = dimension2Field;
        this.dimension3Field = dimension3Field;
    }

    public StockDRMParameters(String itemIdField, String dimension1Field, String dimension2Field, String dimension3Field,
                              String transIdField, String serialField, String batchField, String warehouseField, String locationField) {
        this.itemIdField = itemIdField;
        this.transIdField = transIdField;
        this.dimension1Field = dimension1Field;
        this.dimension2Field = dimension2Field;
        this.dimension3Field = dimension3Field;
        this.serialField = serialField;
        this.batchField = batchField;
        this.warehouseField = warehouseField;
        this.locationField = locationField;
    }

    public String getDimension1Field() {
        return dimension1Field;
    }

    public void setDimension1Field(String dimension1Field) {
        this.dimension1Field = dimension1Field;
    }

    public String getDimension2Field() {
        return dimension2Field;
    }

    public void setDimension2Field(String dimension2Field) {
        this.dimension2Field = dimension2Field;
    }

    public String getDimension3Field() {
        return dimension3Field;
    }

    public void setDimension3Field(String dimension3Field) {
        this.dimension3Field = dimension3Field;
    }

    public String getItemIdField() {
        return itemIdField;
    }

    public void setItemIdField(String itemIdField) {
        this.itemIdField = itemIdField;
    }

    public String getTransIdField() {
        return transIdField;
    }

    public void setTransIdField(String transIdField) {
        this.transIdField = transIdField;
    }

    public String getBatchField() {
        return batchField;
    }

    public void setBatchField(String batchField) {
        this.batchField = batchField;
    }

    public String getSerialField() {
        return serialField;
    }

    public void setSerialField(String serialField) {
        this.serialField = serialField;
    }

    public String getItemReferenceField() {
        return itemReferenceField;
    }

    public void setItemReferenceField(String itemReferenceField) {
        this.itemReferenceField = itemReferenceField;
    }

    public String getLocationField() {
        return locationField;
    }

    public void setLocationField(String locationField) {
        this.locationField = locationField;
    }

    public String getWarehouseField() {
        return warehouseField;
    }

    public void setWarehouseField(String warehouseField) {
        this.warehouseField = warehouseField;
    }
}
