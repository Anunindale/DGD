package emc.base;

import emc.enums.base.entityrelationdiagram.EntityRelationTypes;

/**
 *
 * @author wikus
 */
public class BaseEntityRelation {

    private int level;
    private int relationLevel;
    private String relationType = EntityRelationTypes.MANY.toString();
    private String className;
    private String displayName;
    private String fieldName;
    private String sqlFieldName;
    private String relatedTable;
    private String relatedTableClassName;
    private String relatedField;
    private String relatedSqlField;
    private String relatedRelationType = EntityRelationTypes.MANY.toString();
    private String deleteRelationType;
    private String updateRelationType;

    public BaseEntityRelation() {
    }

    public String getClassName() {
        return className;
    }

    public void setClassName(String className) {
        this.className = className;
    }

    public String getFieldName() {
        return fieldName;
    }

    public void setFieldName(String fieldName) {
        this.fieldName = fieldName;
    }

    public String getDeleteRelationType() {
        return deleteRelationType;
    }

    public void setDeleteRelationType(String deleteRelationType) {
        this.deleteRelationType = deleteRelationType;
    }

    public String getUpdateRelationType() {
        return updateRelationType;
    }

    public void setUpdateRelationType(String updateRelationType) {
        this.updateRelationType = updateRelationType;
    }

    public int getLevel() {
        return level;
    }

    public void setLevel(int level) {
        this.level = level;
    }

    public String getSqlFieldName() {
        return sqlFieldName;
    }

    public void setSqlFieldName(String sqlFieldName) {
        this.sqlFieldName = sqlFieldName;
    }

    public int getRelationLevel() {
        return relationLevel;
    }

    public void setRelationLevel(int relationLevel) {
        this.relationLevel = relationLevel;
    }

    public String getRelationType() {
        return relationType;
    }

    public void setRelationType(String relationType) {
        this.relationType = relationType;
    }

    public String getRelatedField() {
        return relatedField;
    }

    public void setRelatedField(String relatedField) {
        this.relatedField = relatedField;
    }

    public String getRelatedTable() {
        return relatedTable;
    }

    public void setRelatedTable(String relatedTable) {
        this.relatedTable = relatedTable;
    }

    public String getRelatedRelationType() {
        return relatedRelationType;
    }

    public void setRelatedRelationType(String relatedRelationType) {
        this.relatedRelationType = relatedRelationType;
    }

    public String getDisplayName() {
        return displayName;
    }

    public void setDisplayName(String displayName) {
        this.displayName = displayName;
    }

    public String getRelatedTableClassName() {
        return relatedTableClassName;
    }

    public void setRelatedTableClassName(String relatedTableClassName) {
        this.relatedTableClassName = relatedTableClassName;
    }

    public String getRelatedSqlField() {
        return relatedSqlField;
    }

    public void setRelatedSqlField(String relatedSqlField) {
        this.relatedSqlField = relatedSqlField;
    }
}
