package emc.datatypes.base.filepath;

import emc.datatypes.EMCString;

/**
 *
 * @author wikus
 */
public class FilePath extends EMCString {

    /** Creates a new instance of filePath */
    public FilePath() {
        this.setEmcLabel("File Path");
        this.setMandatory(true);
        this.setEditable(true);
        this.setStringSize(256);
        this.setLowerCaseAllowed(true);
    }
}
