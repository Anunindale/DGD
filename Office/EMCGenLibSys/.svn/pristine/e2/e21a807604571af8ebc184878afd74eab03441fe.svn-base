package emc.datatypes.base.fileassociations;

import emc.datatypes.EMCString;

/**
 *
 * @author riaan
 */
public class FileExtension extends EMCString {

    /** Creates a new instance of FileExtension. */
    public FileExtension() {
        this.setEmcLabel("Extension");
        this.setLowerCaseAllowed(true);
    	this.setColumnWidth(40);
        this.setMandatory(true);
    }
}
