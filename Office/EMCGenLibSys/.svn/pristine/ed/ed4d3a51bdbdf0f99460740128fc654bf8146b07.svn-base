package emc.datatypes.sop.salesorderlines;

import emc.datatypes.EMCBigDecimal;

/**
 *
 * @author wikus
 */
public class Price extends EMCBigDecimal {

    public Price() {
        this.setEmcLabel("Price");
        this.setNumberDecimalsDisplay(2);
        this.setNumberDecimalsInput(2);
        this.setMinValue(0);
    	this.setColumnWidth(56);
        this.setMaxValue(Double.MAX_VALUE);
    }
}
