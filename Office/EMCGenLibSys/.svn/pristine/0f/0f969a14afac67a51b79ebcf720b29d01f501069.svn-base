package emc.entity.pop.posting.datasource;

import emc.entity.pop.posting.POPPurchasePostMaster;

/**
 *
 * @author riaan
 */
public class POPPurchasePostMasterDS extends POPPurchasePostMaster {

    private boolean post;
    private boolean print;
    private String quantitySelection;
    private String documentType;
    private String supplierName;
    
    /** Creates a new instance of POPPurchasePostMasterDS */
    public POPPurchasePostMasterDS() {
        setDataSource(true);
    }

    public boolean isPost() {
        return post;
    }

    public void setPost(boolean post) {
        this.post = post;
    }

    public boolean isPrint() {
        return print;
    }

    public void setPrint(boolean print) {
        this.print = print;
    }

    public String getQuantitySelection() {
        return quantitySelection;
    }

    public void setQuantitySelection(String quantitySelection) {
        this.quantitySelection = quantitySelection;
    }

    public String getDocumentType() {
        return documentType;
    }

    public void setDocumentType(String documentType) {
        this.documentType = documentType;
    }

    public String getSupplierName() {
        return supplierName;
    }

    public void setSupplierName(String supplierName) {
        this.supplierName = supplierName;
    }
    
    
}
