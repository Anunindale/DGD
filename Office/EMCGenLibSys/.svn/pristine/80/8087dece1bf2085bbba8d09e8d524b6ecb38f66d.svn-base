package emc.datatypes.inventory.itemmaster;

import emc.datatypes.EMCString;
import emc.entity.inventory.dimensions.InventoryItemDimensionGroup;

/**
 *
 * @author riaan
 */
public class DimensionGroup extends EMCString { 

    /** Creates a new instance of DimensionGroup */
    public DimensionGroup() {
        this.setEmcLabel("Dimension Group");
        this.setColumnWidth(80);
        this.setRelatedTable(InventoryItemDimensionGroup.class.getName());
        this.setRelatedField("itemDimensionGroupId");
        this.setEditable(true);
        this.setMandatory(true);
    }
}
