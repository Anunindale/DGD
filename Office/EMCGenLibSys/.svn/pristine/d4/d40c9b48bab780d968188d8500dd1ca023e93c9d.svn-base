package emc.datatypes.base.numbersequences;

import emc.datatypes.EMCLong;

/**
 *
 * @author riaan
 */
public class MaxNumber extends EMCLong { 

    /** Creates a new instance of MaxNumber */
    public MaxNumber() {
        this.setEmcLabel("Max Value");
        this.setMandatory(true);
        this.setEditable(true);
        this.setMinValue(1);
        this.setMaxValue(100000000);
    }
}
