package emc.entity.inventory.picking.rereservepicklist;

import emc.inventory.ItemReferenceInterface;

/**
 *
 * @author wikus
 */
public class InventoryReReservePickListDS extends InventoryReReservePickList implements ItemReferenceInterface {

    private String itemReference;
    private String itemDescription;

    public InventoryReReservePickListDS() {
        this.setDataSource(true);
    }

    public String getItemDescription() {
        return itemDescription;
    }

    public void setItemDescription(String itemDescription) {
        this.itemDescription = itemDescription;
    }

    public String getItemReference() {
        return itemReference;
    }

    public void setItemReference(String itemReference) {
        this.itemReference = itemReference;
    }
}
