package emc.bus.gl.transactions.logic;

/**
 *
 * @author riaan
 */
public class GLTransactionException extends Exception {

    /**
     * Creates a new instance of GLTransactionException.
     * @param message Exception message.
     */
    public GLTransactionException(String message) {
        super(message);
    }

    /**
     * Creates a new instance of GLTransactionException.
     * @param cause The exception which caused this exception.
     */
    public GLTransactionException(Exception cause) {
        super(cause);
    }

    /**
     * Creates a new instance of GLTransactionException.
     * @param message Exception message.
     * @param cause The exception which caused this exception.
     */
    public GLTransactionException(String message, Exception cause) {
        super(message, cause);
    }
}
