package emc.reports.inventory.ageing;

import emc.entity.inventory.transactions.InventoryTransactions;
import java.util.Date;

/**
 *
 * @author rico
 */
public class ResolverData {
    private InventoryTransactions originalTransaction;
    private double qtyResolved;
    private Date dateResolved;

    public ResolverData(InventoryTransactions o,double qty,Date resDate){
        originalTransaction =o;
        qtyResolved = qty;
        dateResolved = resDate;
    }
    /**
     * @return the originalTransaction
     */
    public InventoryTransactions getOriginalTransaction() {
        return originalTransaction;
    }

    /**
     * @param originalTransaction the originalTransaction to set
     */
    public void setOriginalTransaction(InventoryTransactions originalTransaction) {
        this.originalTransaction = originalTransaction;
    }

    /**
     * @return the qtyResolved
     */
    public double getQtyResolved() {
        return qtyResolved;
    }

    /**
     * @param qtyResolved the qtyResolved to set
     */
    public void setQtyResolved(double qtyResolved) {
        this.qtyResolved = qtyResolved;
    }

    /**
     * @return the dateResolved
     */
    public Date getDateResolved() {
        return dateResolved;
    }

    /**
     * @param dateResolved the dateResolved to set
     */
    public void setDateResolved(Date dateResolved) {
        this.dateResolved = dateResolved;
    }

}
